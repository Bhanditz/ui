<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE product-profile
        SYSTEM "http://helpserver.labs.intellij.net/help/product-profile.dtd">
<product-profile id="uip"
                 name="IntelliJ UI Private Guidelines"
                 start-page="combobox.xml" icon="uiForm.png">

    <toc-element toc-title="Controls" include-id="controls">
        <toc-element id="input_field.xml"/>
        <toc-element id="progress_bar.xml"/>
        <toc-element id="text_area.xml"/>
        <toc-element filter="private" id="checkbox.xml"/>
        <toc-element filter="private" id="combobox.xml"/>
        <toc-element filter="private" id="radiobutton.xml"/>
        <toc-element filter="private" id="dropdown.xml"/>
    </toc-element>

    <toc-element toc-title="Principles" include-id="principles">
        <toc-element id="numeric_fields.xml" filter="private"/>
        <toc-element id="writing.xml" filter="private">
            <toc-element id="capitalization.xml" filter="private"/>
        </toc-element>
    </toc-element>

</product-profile>
